<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	         http://www.springframework.org/schema/beans/spring-beans.xsd
		 http://www.springframework.org/schema/context
		 http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <bean id="person" class="site.xzq_xu.test.ioc.bean.Person">
        <property name="name" value="李四"/>
        <property name="cat" ref="cat"/>
    </bean>

    <bean id="cat" class="site.xzq_xu.test.ioc.bean.Cat">
        <property name="name" value="Jerry"/>
        <property name="age" value="3"/>

    </bean>


    <bean class="site.xzq_xu.test.ioc.common.CustomBeanFactoryPostProcessor"/>
    <bean class="site.xzq_xu.test.ioc.common.CustomBeanPostProcessor"/>

</beans>